<?xml version="1.0" encoding="UTF-8"?>

<decorators defaultdir="/WEB-INF/decorators">
    <!-- 在excludes元素下指定的页面将不会由SiteMesh来装饰 -->
    <excludes>
    	<pattern>/*.html</pattern>
    </excludes>
    
    <decorator name="Main" page="Main.jsp">
        <pattern>/*</pattern>
    </decorator>

</decorators>